{
  "name": "home",
  "description": "Okkema Labs Home Page",
  "type": "module",
  "version": "0.0.3",
  "scripts": {
    "build": "npm run build:astro",
    "build:astro": "astro build",
    "build:storybook": "storybook build",
    "start": "npm run start:wrangler",
